{
    "name": "shibomb/filament-simple-memo",
    "description": "This is my package filament-simple-memo",
    "keywords": [
        "shibomb",
        "laravel",
        "filament-simple-memo"
    ],
    "homepage": "https://github.com/shibomb/filament-simple-memo",
    "support": {
        "issues": "https://github.com/shibomb/filament-simple-memo/issues",
        "source": "https://github.com/shibomb/filament-simple-memo"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "SHIBAHARA Hiroki",
            "email": "shibomb@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Shibomb\\FilamentTodo\\": "src/",
            "Shibomb\\FilamentTodo\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shibomb\\FilamentTodo\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shibomb\\FilamentTodo\\FilamentTodoServiceProvider"
            ],
            "aliases": {
                "FilamentTodo": "Shibomb\\FilamentTodo\\Facades\\FilamentTodo"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
